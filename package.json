{
  "name": "boilerplate-wordpress",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "delete-manifest": "npx rimraf dist/.vite/manifest.json",
    "custom-blocks-dev": "wp-scripts start custom-blocks/index.js",
    "custom-blocks-build": "wp-scripts build custom-blocks/index.js",
    "imagemin-build": "node imagemin.mjs src/assets/images/**/* dist/assets/images",
    "dev": "yarn delete-manifest && vite",
    "build": "vite build && yarn custom-blocks-build && yarn imagemin-build"
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.13.8",
    "@alpinejs/focus": "^3.13.8",
    "@alpinejs/mask": "^3.13.8",
    "@caneara/iodine": "^8.5.0",
    "alpinejs": "^3.13.8",
    "clsx": "^2.1.0",
    "jquery": "^3.7.1",
    "recaptcha-v3": "^1.10.0",
    "tailwind-merge": "^2.2.2",
    "vanilla-lazyload": "^19.1.3"
  },
  "devDependencies": {
    "@ixkaito/imagemin": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@wordpress/icons": "^9.46.0",
    "@wordpress/scripts": "^27.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.0.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "rimraf": "^5.0.5",
    "tailwind-bootstrap-grid": "^5.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vite-plugin-live-reload": "^3.0.3",
    "vite-plugin-static-copy": "^1.0.2"
  }
}
