    "alpinejs": "^3.13.7",
    "clsx": "^2.1.0",
    "jquery": "^3.7.1",
    "recaptcha-v3": "^1.10.0",
    "tailwind-merge": "^2.2.1",
    "vanilla-lazyload": "^17.8.8"
  },
  "devDependencies": {
    "@ixkaito/imagemin": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@wordpress/icons": "^9.44.0",
    "@wordpress/scripts": "^27.4.0",
    "autoprefixer": "^10.4.18",
